@using SX.WebCore.ViewModels.GitHub
@model  SxVMGitHubLabel[]

@if (Model.Any())
{
    SxVMGitHubLabel item = null;
    <ul class="list-unstyled list-inline">
        @for (int i = 0; i < Model.Length; i++)
        {
            item = Model[i];
            <li>
                <span class="gh-label" style="background-color:#@item.Color">@item.Name</span>
            </li>
        }
    </ul>
}
